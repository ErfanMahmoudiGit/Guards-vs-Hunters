<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="700.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GetNameController">
    <children>
        <ImageView fitHeight="700.0" fitWidth="1167.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Images/backGround.jpg" />
            </image>
        </ImageView>
        <TextField fx:id="name_enter_field" layoutX="437.0" layoutY="94.0" onAction="#name_entered">
            <font>
                <Font size="20.0" />
            </font>
        </TextField>
        <Button fx:id="exit_game_button" layoutX="532.0" layoutY="434.0" mnemonicParsing="false" onAction="#exit" text="Exit" textFill="#c30909">
            <font>
                <Font size="20.0" />
            </font></Button>
        <Button fx:id="main_menu" layoutX="460.0" layoutY="379.0" mnemonicParsing="false" onAction="#mainMenu" text="Back To Main Menu" textFill="#c30909">
            <font>
                <Font size="20.0" />
            </font></Button>
        <Text layoutX="424.0" layoutY="72.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please Enter Your Name">
            <font>
                <Font size="26.0" />
            </font>
        </Text>
    </children>
</AnchorPane>
